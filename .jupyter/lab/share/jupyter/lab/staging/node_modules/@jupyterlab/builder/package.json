    "@lumino/domutils": "^1.8.0",
    "@lumino/dragdrop": "^1.13.0",
    "@lumino/messaging": "^1.10.0",
    "@lumino/properties": "^1.8.0",
    "@lumino/signaling": "^1.10.0",
    "@lumino/virtualdom": "^1.14.0",
    "@lumino/widgets": "^1.37.1",
    "ajv": "^6.12.3",
